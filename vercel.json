{
	"name": "backend-capstone-umkmgrow",
	"version": 2,
	"public": true,
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		},
		{
			"src": "public/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js"
		},
		{
			"src": "/(.+\\.[a-z]+)$",
			"dest": "/public/$1"
		}
	]
}
