{
	"version": 2,
	"builds": [
		{
			"src": "src/server.js",
			"use": "@vercel/node"
		},
		{
			"src": "public/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/((?!.*\\.\\w+$).*)",
			"dest": "/src/server.js"
		},
		{
			"src": "/(.+\\.[a-z]+)$",
			"dest": "/public/$1"
		}
	]
}
